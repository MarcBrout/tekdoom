#include "tekdoom.h"

void			draw_scope(t_param *arg)
{
  t_color		*color;
  t_bunny_position	pos;
  t_bunny_position	end;
  int			i;

  color = (t_color*)arg->inter.gun.scope;
  pos.y = 0;
  i = 0;
}
